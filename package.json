{
  "name": "api-calls-buffer",
  "version": "1.0.5",
  "description": "When your api object takes a few ticks to initialize, use this package to buffer its usage then play it on the real one!",
  "main": "index.js",
  "homepage": "https://github.com/iBaryo/api-calls-buffer",
  "scripts": {
    "build": "tsc index.ts --target es2016 --module commonjs --sourcemap --declaration",
    "prepublish": "npm test",
    "start": "ts-node example.ts",
    "test": "npm run build && jasmine-ts \"spec/*spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iBaryo/api-calls-buffer.git"
  },
  "author": "Or Bar Yosef",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "jasmine-ts": "^0.2.1",
    "typescript": "^2.5.3"
  }
}
